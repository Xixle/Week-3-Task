<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.AdminEntity" table="admin" schema="mambodb">
        <id name="userId">
            <column name="userId" sql-type="int(11)"/>
        </id>
        <property name="adminName">
            <column name="adminName" sql-type="varchar(32)" length="32"/>
        </property>
        <property name="emaii">
            <column name="emaii" sql-type="varchar(32)" length="32"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(32)" length="32"/>
        </property>
        <property name="loginStatus">
            <column name="loginStatus" sql-type="varchar(32)" length="32"/>
        </property>
    </class>
</hibernate-mapping>